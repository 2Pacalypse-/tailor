  "private": true,
  "scripts": {
    "dev": "node config/client/startup/client.js",
    "build": "npm run clean && npm run build:dist",
    "build:dist": "cross-env NODE_ENV=production webpack --config config/client/webpack/prod.js --progress --hide-modules",
    "e2e": "node client/test/e2e/runner.js",
    "unit:client": "karma start karma.conf.js",
    "test:client": "npm run unit:client && npm run e2e",
    "clean": "rimraf dist",
    "lint:client": "eslint --ext .js,.vue config client/test/e2e/specs client/test/unit/specs client/components client/store",
    "lint:server": "eslint --ext .js server",
    "lint": "npm run lint:client && npm run lint:server",
    "dev:server": "nodemon --ignore client/ --exec \"node -r dotenv/config\" ./server"
  },
