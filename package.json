{
  "name": "tailor",
  "version": "1.0.0",
  "description": "Authoring platform",
  "author": "Damir Perisic <dperisic@extensionengine.com>",
  "private": true,
  "browserslist": ["last 2 versions", "ie 11"],
  "scripts": {
    "dev:client": "poi --config config/client/poi.config.js",
    "build": "poi build --config config/client/poi.config.js",
    "e2e": "node client/test/e2e/runner.js",
    "unit:client": "echo \"Error: no unit tests specified\" && exit 0",
    "test:client": "npm run unit:client && npm run e2e",
    "clean": "rimraf dist",
    "lint:client": "eslint --ext .js,.vue config/client client/test/e2e/specs client/test/unit/specs client/components client/store",
    "lint:server": "eslint --ext .js server config/server",
    "lint": "npm run lint:client && npm run lint:server",
    "dev:server": "nodemon --ignore client/ --exec \"node -r dotenv/config\" ./server | bunyan",
    "debug:server": "node --inspect-brk -r dotenv/config ./server",
    "doc:server": "jsdoc --pedantic --destination ./server/_doc_ --recurse ./server",
    "unit:server": "mocha --recursive \"./server/**/*.spec.js\"",
    "review:server": "npm prune && npm install && npm run unit:server && npm run lint:server",
    "postinstall": "if [ $BUILD_ASSETS ]; then npm run build; fi",
    "db:seed": "node -r dotenv/config ./server/script/dbSeed.js",
    "db:reset": "node -r dotenv/config ./server/script/dbReset.js",
    "migrate": "node ./server/script/migrate.js",
    "add:admin": "node -r dotenv/config ./server/script/addAdmin.js",
    "fix:activities": "node -r dotenv/config ./server/script/fixActivities.js",
    "fix:assessments": "node -r dotenv/config ./server/script/fixAssessments.js",
    "fix:images": "node -r dotenv/config ./server/script/fixImageUrls.js",
    "fix:stats": "node -r dotenv/config ./server/script/fixStats.js",
    "fix:mark-detached": "node -r dotenv/config ./server/script/markDetached.js",
    "fix:mq": "node -r dotenv/config ./server/script/fixMatchingQuestionData.js",
    "fix:nr": "node -r dotenv/config ./server/script/fixNRAssessments.js",
    "fix:embeds": "node -r dotenv/config ./server/script/fixEmbeds.js",
    "update:description": "node -r dotenv/config ./server/script/updateDescription.js",
    "integration:add": "node -r dotenv/config ./server/script/addIntegration.js",
    "integration:token": "node -r dotenv/config ./server/script/generateIntegrationToken.js",
    "start": "node -r dotenv/config ./server"
  },
  "dependencies": {
    "aws-sdk": "2.7.27",
    "axios": "^0.15.2",
    "bcryptjs": "^2.3.0",
    "blob-util": "^1.2.1",
    "bluebird": "^3.4.6",
    "body-parser": "^1.15.2",
    "bootstrap-sass": "^3.3.7",
    "bootswatch": "^3.3.7",
    "bunyan": "^1.8.5",
    "cors": "^2.8.1",
    "cropperjs": "^1.0.0-beta.1",
    "cuid": "^1.3.8",
    "dotenv": "^2.0.0",
    "email-regex": "^1.0.0",
    "emailjs": "^1.0.8",
    "exceljs": "^0.4.10",
    "express": "^4.14.0",
    "express-promise-router": "^1.1.1",
    "fecha": "^2.3.0",
    "focus-trap": "^2.3.1",
    "http-errors": "^1.5.1",
    "http-status-codes": "^1.1.6",
    "is-url": "^1.2.2",
    "joi": "^10.0.1",
    "jquery": "^3.1.1",
    "jsonwebtoken": "^7.2.1",
    "jszip": "^3.1.3",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.2",
    "mdi": "^1.8.36",
    "mime-types": "^2.1.14",
    "minimist": "^1.2.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "pluralize": "^7.0.0",
    "promise-queue": "^2.2.3",
    "rcfile": "^1.0.3",
    "save-as": "^0.1.8",
    "sequelize": "^3.3.0",
    "string-template": "^1.0.0",
    "to-case": "^2.0.0",
    "truncate": "^2.0.0",
    "url-join": "^1.1.0",
    "vee-validate": "^2.0.0-rc.8",
    "videojs-vimeo": "^2.0.2",
    "videojs-youtube": "^2.4.0",
    "vue": "^2.5.2",
    "vue-color": "^2.3.0",
    "vue-focus": "^2.1.0",
    "vue-infinite-loading": "^2.2.1",
    "vue-infinite-scroll": "^2.0.0",
    "vue-multiselect": "^2.0.2",
    "vue-quill-editor": "^2.2.4",
    "vue-router": "^2.0.3",
    "vue-video-player": "^3.1.4",
    "vuedraggable": "^2.14.1",
    "vuex": "^2.0.0",
    "vuex-module": "^0.3.0",
    "vuex-router-sync": "^3.0.0",
    "yup": "^0.21.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "imports-loader": "^0.7.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "poi": "^9.4.3",
    "poi-preset-eslint": "^9.0.1",
    "sass-loader": "^6.0.6",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "stylelint-processor-html": "^1.0.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "swagger-jsdoc": "^1.8.2",
    "swagger-ui": "^2.2.10",
    "val-loader": "^1.0.2",
    "vue-template-compiler": "^2.5.2",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "engines": {
    "node": ">= 8.8.0",
    "npm": ">= 5.4.2"
  }
}
